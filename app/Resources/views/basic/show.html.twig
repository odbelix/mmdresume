{% extends "ResumeBundle:Default:panel-backend.html.twig" %}

{% block maincontent %}
<div class="row">
    <div class="col-lg-12">
    <h1 class="page-header">Título Docente Educación Básica</h1>
    </div>
    <!-- /.col-lg-12 -->
</div>
<div class="row">
    <div class="col-lg-6">
      <div class="panel panel-default">
        <div class="panel-heading">
          Título Docente Educación Básica # {{ basic.id }}
        </div>
        <div class="panel-body">
            <table class="table table-bordered">
                <tbody>
                    <tr>
                        <th>#</th>
                        <td>{{ basic.id }}</td>
                    </tr>
                    <tr>
                        <th>Nombre</th>
                        <td>{{ basic.name }}</td>
                    </tr>
                    <tr>
                      <td colspan="2">
                      <div class="btn-group button-in-table" role="group" aria-label="...">
                          <a class="btn btn-default"  href="{{ path('panel_basic_index') }}">Volver</a>
                          <a class="btn btn-default" href="{{ path('panel_basic_edit', { 'id': basic.id }) }}">Editar</a>
                      </div>
                      </td>
                    </tr>
                </tbody>
            </table>
            </div>
            <div class="panel-footer">
            {{ form_start(delete_form) }}
                <input class="btn btn-danger" type="submit" value="Eliminar">
            {{ form_end(delete_form) }}
            </div>
        </div>
        <div class="col-lg-6">
        </div>
</div>
{% endblock %}
