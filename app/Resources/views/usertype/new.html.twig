{% extends "ResumeBundle:Default:panel-backend.html.twig" %}
{% block maincontent %}
{{ include ('usertype/title.html.twig') }}
<div class="row">
    <div class="col-lg-6">
        <div class="panel panel-default">
            <div class="panel-heading">
            Nuevo Tipo de Usuario
            </div>
            <div class="panel-body">

    {{ form_start(form) }}

    <div class="form-group">
    <label>Nombre</label>
        <p class="text-danger">{{ form_errors(form.name) }}</p>
        {{ form_widget(form.name,{ 'attr': {'class': 'form-control'} }) }}
    </div>
    <div class="form-group">
    <label>Detalle</label>
        <p class="text-danger">{{ form_errors(form.detail) }}</p>
        {{ form_widget(form.detail,{ 'attr': {'class': 'form-control'} }) }}
    </div>
        <div class="form-group">
                <input class="btn btn-default" type="submit" value="Agregar" />
        </div>
    {{ form_end(form) }}

    </div>
  </div>
  </div>
  <div class="col-lg-6">
  </div>
</div>

{% endblock %}
